# Lumenore API Configuration
# Copy this file to .env and fill in your actual credentials

# These are used for the user-login endpoint (https://preview.lumenore.com)

# Required: Your Lumenore API key/token (Bearer token)
LUMENORE_API_KEY="your_api_key_here"

# OR

# Required: Client authentication credentials
LUMENORE_CLIENT_ID="your_client_id_here"
LUMENORE_CLIENT_SECRET="your_client_secret_here"

# Optional: Custom server URL (defaults to https://preview.lumenore.com)
SERVER_URL="https://preview.lumenore.com"

Optional: Enable debug mode
DEBUG=true
